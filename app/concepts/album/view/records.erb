<%= cell(Application::Cell::Header, 'Albums') %>

<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
    </tr>
  </thead>
  <tbody>
    <% model.each do |album| %>
      <%= cell(Album::Cell::Row, album) %>
    <% end %>
  </tbody>
</table>

<%= link_to('New Album', new_album_path) %>